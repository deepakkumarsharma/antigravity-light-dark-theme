{
  "$schema": "vscode://schemas/color-theme",
  "name": "Antigravity Vesper Pro Light+",
  "type": "light",
  "colors": {
    "editor.background": "#FCFCFE",
    "editor.foreground": "#101015",
    "activityBar.background": "#F5F5FA",
    "activityBar.foreground": "#101015",
    "activityBar.border": "#E5E5EF",
    "sideBar.background": "#F5F5FA",
    "sideBar.foreground": "#4B5563",
    "sideBar.border": "#E5E5EF",
    "sideBarTitle.foreground": "#101015",
    "statusBar.background": "#101015",
    "statusBar.foreground": "#FCFCFE",
    "statusBar.border": "#101015",
    "editorGroupHeader.tabsBackground": "#F5F5FA",
    "editorGroupHeader.tabsBorder": "#E5E5EF",
    "tab.activeBackground": "#FCFCFE",
    "tab.activeForeground": "#101015",
    "tab.activeBorder": "#F97316",
    "tab.activeBorderTop": "#F97316",
    "tab.inactiveBackground": "#F5F5FA",
    "tab.inactiveForeground": "#94A3B8",
    "tab.border": "#E5E5EF",
    "editor.lineHighlightBackground": "#F0F0FF",
    "editorCursor.foreground": "#F97316",
    "editor.selectionBackground": "#0D948825",
    "editor.selectionHighlightBackground": "#0D948815",
    "editorBracketHighlight.foreground1": "#F97316",
    "editorBracketHighlight.foreground2": "#0D9488",
    "editorBracketHighlight.foreground3": "#64748B",
    "list.activeSelectionBackground": "#E5E5EF",
    "list.activeSelectionForeground": "#101015",
    "list.inactiveSelectionBackground": "#F5F5FA",
    "list.hoverBackground": "#F5F5FA",
    "sideBarTitle.background": "#F5F5FA",
    "breadcrumb.foreground": "#64748B",
    "breadcrumb.activeSelectionForeground": "#101015",
    "breadcrumb.focusForeground": "#101015",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.border": "#E5E5EF",
    "editorSuggestWidget.foreground": "#101015",
    "editorSuggestWidget.highlightForeground": "#F97316",
    "editorSuggestWidget.selectedBackground": "#EAEAEF",
    "editorSuggestWidget.selectedForeground": "#101015",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#E5E5EF",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#E5E5EF",
    "editorWidget.foreground": "#101015",
    "tree.indentGuidesStroke": "#E5E5EF",
    "editorIndentGuide.background1": "#E5E5EF",
    "editorIndentGuide.activeBackground1": "#D1D5DB",
    "editorError.foreground": "#DC2626",
    "editorError.background": "#EF444410",
    "editorError.border": "#00000000",
    "editorOverviewRuler.errorForeground": "#DC2626",
    "list.errorForeground": "#DC2626"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "Main Logic (Bold)",
      "scope": [
        "keyword.control.return",
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.trycatch",
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#F97316",
        "fontStyle": "bold"
      }
    },
    {
      "name": "General Keywords & Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword",
        "keyword.operator.new",
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#F97316"
      }
    },
    {
      "name": "Function Definitions (Bold)",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#0D9488",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Methods & Calls",
      "scope": [
        "variable.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Classes & Structs (Bold)",
      "scope": [
        "entity.name.type.class",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#0D9488",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other Types",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language.special.self",
        "meta.definition.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#101015"
      }
    },
    {
      "name": "Strings & Template Literals",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#B45309"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#EA580C"
      }
    },
    {
      "name": "Properties & Object Keys",
      "scope": [
        "meta.object-literal.key",
        "variable.other.property",
        "support.type.property-name",
        "entity.other.attribute-name",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Punctuation & Operators",
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#64748B"
      }
    }
  ],
  "semanticHighlighting": true
}