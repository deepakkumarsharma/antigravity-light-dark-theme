{
  "$schema": "vscode://schemas/color-theme",
  "name": "Antigravity Vesper Pro Light+",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#101010",
    "activityBar.background": "#FFFFFF",
    "activityBar.foreground": "#101010",
    "activityBar.border": "#EEEEEE",
    "sideBar.background": "#FAFAFA",
    "sideBar.foreground": "#4B5563",
    "sideBar.border": "#EEEEEE",
    "sideBarTitle.foreground": "#101010",
    "statusBar.background": "#101010",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.border": "#101010",
    "editorGroupHeader.tabsBackground": "#FFFFFF",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#101010",
    "tab.inactiveBackground": "#FAFAFA",
    "tab.inactiveForeground": "#9CA3AF",
    "tab.border": "#EEEEEE",
    "editor.lineHighlightBackground": "#F9FAFB",
    "editorCursor.foreground": "#EA580C",
    "editor.selectionBackground": "#EA580C15",
    "editorBracketHighlight.foreground1": "#EA580C",
    "editorBracketHighlight.foreground2": "#0D9488",
    "editorBracketHighlight.foreground3": "#71717A",
    "list.activeSelectionBackground": "#F3F4F6",
    "list.activeSelectionForeground": "#101010",
    "list.inactiveSelectionBackground": "#F9FAFB",
    "list.hoverBackground": "#F9FAFB",
    "sideBarTitle.background": "#FAFAFA",
    "breadcrumb.foreground": "#9CA3AF",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.border": "#EEEEEE",
    "editorSuggestWidget.foreground": "#101010",
    "editorSuggestWidget.highlightForeground": "#EA580C",
    "editorSuggestWidget.selectedBackground": "#F3F4F6",
    "editorSuggestWidget.selectedForeground": "#101010",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#EEEEEE",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#EEEEEE",
    "editorWidget.foreground": "#101010",
    "editorError.foreground": "#DC2626",
    "editorError.background": "#EF444415",
    "editorError.border": "#00000000",
    "editorOverviewRuler.errorForeground": "#DC2626",
    "list.errorForeground": "#DC2626"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A1A1AA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Structural Keywords (Teal)",
      "scope": [
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.default",
        "keyword.control.flow",
        "keyword.control.return"
      ],
      "settings": {
        "foreground": "#0D9488",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Declaration Keywords (Grey)",
      "scope": [
        "storage.type.interface",
        "storage.type.type",
        "storage.type.const",
        "storage.type.let",
        "storage.type.var",
        "storage.type.class",
        "storage.type.function",
        "keyword.other",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#71717A"
      }
    },
    {
      "name": "Definitions & Tags (Orange)",
      "scope": [
        "entity.name.function",
        "entity.name.type",
        "entity.name.tag",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#EA580C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Props & Parameters (Teal)",
      "scope": [
        "variable.parameter",
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Object Keys (Grey)",
      "scope": [
        "meta.object-literal.key",
        "variable.other.property",
        "support.type.property-name",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "Strings & Constants",
      "scope": ["string", "punctuation.definition.string", "constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#B45309"
      }
    },
    {
      "name": "Content (Premium Black)",
      "scope": [
        "variable",
        "variable.other",
        "variable.language.special.self",
        "meta.definition.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "name": "Punctuation & Operators (Grey)",
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#94A3B8"
      }
    }
  ],
  "semanticHighlighting": true
}