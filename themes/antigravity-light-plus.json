{
  "$schema": "vscode://schemas/color-theme",
  "name": "Antigravity Vesper Pro Light+",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#101010",
    "activityBar.background": "#FFFFFF",
    "activityBar.foreground": "#101010",
    "activityBar.border": "#EEEEEE",
    "sideBar.background": "#FAFAFA",
    "sideBar.foreground": "#4B5563",
    "sideBar.border": "#EEEEEE",
    "sideBarTitle.foreground": "#101010",
    "statusBar.background": "#101010",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.border": "#101010",
    "editorGroupHeader.tabsBackground": "#FFFFFF",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#101010",
    "tab.inactiveBackground": "#FAFAFA",
    "tab.inactiveForeground": "#9CA3AF",
    "tab.border": "#EEEEEE",
    "editor.lineHighlightBackground": "#F9FAFB",
    "editorCursor.foreground": "#EA580C",
    "editor.selectionBackground": "#EA580C25",
    "editorBracketHighlight.foreground1": "#EA580C",
    "editorBracketHighlight.foreground2": "#0D9488",
    "editorBracketHighlight.foreground3": "#6366F1",
    "list.activeSelectionBackground": "#F3F4F6",
    "list.activeSelectionForeground": "#101010",
    "list.inactiveSelectionBackground": "#F9FAFB",
    "list.hoverBackground": "#F9FAFB",
    "sideBarTitle.background": "#FAFAFA",
    "breadcrumb.foreground": "#9CA3AF",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.border": "#EEEEEE",
    "editorSuggestWidget.foreground": "#101010",
    "editorSuggestWidget.highlightForeground": "#0D9488",
    "editorSuggestWidget.selectedBackground": "#F3F4F6",
    "editorSuggestWidget.selectedForeground": "#101010",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#EEEEEE",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#EEEEEE",
    "editorWidget.foreground": "#101010",
    "editorError.foreground": "#DC2626",
    "editorError.background": "#EF444415",
    "editorError.border": "#00000000",
    "editorOverviewRuler.errorForeground": "#DC2626",
    "list.errorForeground": "#DC2626"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A1A1AA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Teal Keywords (Def, Return, Import)",
      "scope": [
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.default",
        "keyword.control.python",
        "storage.type.function.python",
        "keyword.control.flow.python",
        "keyword.control.flow.return.python",
        "storage.type.class",
        "storage.type.function",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#0D9488",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other Keywords (Logic)",
      "scope": ["keyword", "storage.type", "storage.modifier", "punctuation.definition.keyword"],
      "settings": {
        "foreground": "#71717A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Definitions",
      "scope": ["entity.name.function", "entity.name.type.class"],
      "settings": {
        "foreground": "#EA580C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function & Method Calls",
      "scope": [
        "variable.function",
        "support.function",
        "meta.function-call.generic",
        "meta.method-call.generic",
        "meta.function-call.python",
        "meta.method-call.python"
      ],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string", "constant.character.escape"],
      "settings": {
        "foreground": "#B45309"
      }
    },
    {
      "name": "Numbers, Constants",
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Eye-Catching Parameters",
      "scope": [
        "variable.parameter",
        "variable.parameter.function-call",
        "variable.parameter.function",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#111827",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables, Objects",
      "scope": ["variable", "variable.other", "variable.other.property", "variable.language.special.self"],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "name": "Punctuation, Operators",
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#94A3B8"
      }
    }
  ],
  "semanticHighlighting": true
}