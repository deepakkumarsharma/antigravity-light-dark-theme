{
  "$schema": "vscode://schemas/color-theme",
  "name": "Antigravity Vesper Pro Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#101010",
    "editor.foreground": "#A0A0A0",
    "activityBar.background": "#101010",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.border": "#101010",
    "sideBar.background": "#101010",
    "sideBar.foreground": "#707070",
    "sideBar.border": "#1A1A1A",
    "sideBarTitle.foreground": "#A0A0A0",
    "statusBar.background": "#101010",
    "statusBar.foreground": "#707070",
    "statusBar.border": "#1A1A1A",
    "editorGroupHeader.tabsBackground": "#101010",
    "tab.activeBackground": "#101010",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#101010",
    "tab.inactiveForeground": "#404040",
    "tab.border": "#101010",
    "editor.lineHighlightBackground": "#1A1A1A",
    "editorCursor.foreground": "#FFCFA0",
    "editor.selectionBackground": "#FFFFFF1A",
    "editorBracketHighlight.foreground1": "#FF7A59",
    "editorBracketHighlight.foreground2": "#00F5D4",
    "editorBracketHighlight.foreground3": "#FFCFA0",
    "list.activeSelectionBackground": "#FFFFFF1A",
    "list.inactiveSelectionBackground": "#FFFFFF0D",
    "sideBarTitle.background": "#101010",
    "breadcrumb.foreground": "#404040",
    "editorSuggestWidget.background": "#1C1C1C",
    "editorSuggestWidget.border": "#333333",
    "editorSuggestWidget.foreground": "#E0E0E0",
    "editorSuggestWidget.highlightForeground": "#FFCFA0",
    "editorSuggestWidget.selectedBackground": "#2D2D2D",
    "editorHoverWidget.background": "#1C1C1C",
    "editorHoverWidget.border": "#333333",
    "editorWidget.background": "#1C1C1C",
    "editorWidget.border": "#333333",
    "editorWidget.foreground": "#E0E0E0",
    "editorError.foreground": "#F87171",
    "editorError.background": "#EF444420",
    "editorError.border": "#00000000",
    "editorOverviewRuler.errorForeground": "#F87171",
    "list.errorForeground": "#F87171"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#505050",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier", "punctuation.definition.keyword"],
      "settings": {
        "foreground": "#FF7A59"
      }
    },
    {
      "scope": ["entity.name.type.class", "entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#00F5D4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "variable.function"],
      "settings": {
        "foreground": "#00F5D4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string", "constant.character.escape"],
      "settings": {
        "foreground": "#FFCFA0"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#FFCFA0"
      }
    },
    {
      "scope": ["variable.language.special.self", "variable.parameter", "variable.other.property"],
      "settings": {
        "foreground": "#FFD8B1"
      }
    },
    {
      "scope": ["variable.other", "variable.parameter.function-call"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#808080"
      }
    }
  ],
  "semanticHighlighting": true
}