{
  "$schema": "vscode://schemas/color-theme",
  "name": "Antigravity Vesper Pro Light",
  "type": "light",
  "colors": {
    "editor.background": "#F7F7FF",
    "editor.foreground": "#101015",
    "activityBar.background": "#F7F7FF",
    "activityBar.foreground": "#101015",
    "activityBar.border": "#E5E5EF",
    "sideBar.background": "#F0F0F7",
    "sideBar.foreground": "#4B5563",
    "sideBar.border": "#E5E5EF",
    "sideBarTitle.foreground": "#101015",
    "statusBar.background": "#F7F7FF",
    "statusBar.foreground": "#4B5563",
    "statusBar.border": "#E5E5EF",
    "editorGroupHeader.tabsBackground": "#F0F0F7",
    "tab.activeBackground": "#F7F7FF",
    "tab.activeForeground": "#101015",
    "tab.inactiveBackground": "#F0F0F7",
    "tab.inactiveForeground": "#94A3B8",
    "tab.border": "#E5E5EF",
    "editor.lineHighlightBackground": "#EFEFF9",
    "editorCursor.foreground": "#F97316",
    "editor.selectionBackground": "#F9731620",
    "editorBracketHighlight.foreground1": "#F97316",
    "editorBracketHighlight.foreground2": "#0D9488",
    "editorBracketHighlight.foreground3": "#64748B",
    "list.activeSelectionBackground": "#EAEAEF",
    "list.activeSelectionForeground": "#101015",
    "list.inactiveSelectionBackground": "#F0F0F7",
    "list.hoverBackground": "#F0F0F7",
    "sideBarTitle.background": "#F0F0F7",
    "breadcrumb.foreground": "#64748B",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.border": "#E5E5EF",
    "editorSuggestWidget.foreground": "#101015",
    "editorSuggestWidget.highlightForeground": "#F97316",
    "editorSuggestWidget.selectedBackground": "#EAEAEF",
    "editorSuggestWidget.selectedForeground": "#101015",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#E5E5EF",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#E5E5EF",
    "editorWidget.foreground": "#101015",
    "editorError.foreground": "#DC2626",
    "editorError.background": "#EF444410",
    "editorError.border": "#00000000",
    "editorOverviewRuler.errorForeground": "#DC2626",
    "list.errorForeground": "#DC2626"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "Keywords & Flow (Orange)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#F97316",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions & Types (Peppermint)",
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function",
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#0D9488",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings & Constants",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.numeric",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#B45309"
      }
    },
    {
      "name": "Variables & Parameters (Premium Black)",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language.special.self",
        "meta.definition.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#101015"
      }
    },
    {
      "name": "Object Keys & Properties",
      "scope": [
        "meta.object-literal.key",
        "variable.other.property",
        "support.type.property-name",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Punctuation & Operators",
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#64748B"
      }
    }
  ],
  "semanticHighlighting": true
}