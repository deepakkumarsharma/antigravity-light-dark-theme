{
  "$schema": "vscode://schemas/color-theme",
  "name": "Antigravity Vesper Pro Light",
  "type": "light",
  "colors": {
    "editor.background": "#FBFBFB",
    "editor.foreground": "#24292F",
    "activityBar.background": "#FBFBFB",
    "activityBar.foreground": "#24292F",
    "activityBar.border": "#E1E4E8",
    "sideBar.background": "#F8F8F8",
    "sideBar.foreground": "#57606A",
    "sideBar.border": "#E1E4E8",
    "sideBarTitle.foreground": "#24292F",
    "statusBar.background": "#FBFBFB",
    "statusBar.foreground": "#57606A",
    "statusBar.border": "#E1E4E8",
    "editorGroupHeader.tabsBackground": "#FBFBFB",
    "tab.activeBackground": "#FBFBFB",
    "tab.activeForeground": "#24292F",
    "tab.inactiveBackground": "#F8F8F8",
    "tab.inactiveForeground": "#57606A",
    "tab.border": "#E1E4E8",
    "editor.lineHighlightBackground": "#F6F8FA",
    "editorCursor.foreground": "#CF222E",
    "editor.selectionBackground": "#0969DA20",
    "editorBracketHighlight.foreground1": "#CF222E",
    "editorBracketHighlight.foreground2": "#0550AE",
    "editorBracketHighlight.foreground3": "#116329",
    "list.activeSelectionBackground": "#E1E4E8",
    "list.activeSelectionForeground": "#24292F",
    "list.inactiveSelectionBackground": "#F0F0F0",
    "list.hoverBackground": "#F6F8FA",
    "sideBarTitle.background": "#F8F8F8",
    "breadcrumb.foreground": "#57606A",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.border": "#E1E4E8",
    "editorSuggestWidget.foreground": "#24292F",
    "editorSuggestWidget.highlightForeground": "#CF222E",
    "editorSuggestWidget.selectedBackground": "#E1E4E8",
    "editorSuggestWidget.selectedForeground": "#24292F",
    "editorSuggestWidget.focusHighlightForeground": "#24292F",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#E1E4E8",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#E1E4E8",
    "editorWidget.foreground": "#24292F"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6E7781",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier", "punctuation.definition.keyword"],
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type.class", "entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#0550AE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "variable.function"],
      "settings": {
        "foreground": "#0550AE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string", "constant.character.escape"],
      "settings": {
        "foreground": "#116329"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "scope": ["variable.language.special.self", "variable.parameter", "variable.other.property"],
      "settings": {
        "foreground": "#5A4E93"
      }
    },
    {
      "scope": ["variable.other", "variable.parameter.function-call"],
      "settings": {
        "foreground": "#24292F"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "keyword.operator"],
      "settings": {
        "foreground": "#57606A"
      }
    }
  ],
  "semanticHighlighting": true
}